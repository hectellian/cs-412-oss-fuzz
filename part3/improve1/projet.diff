@@ -0,0 +1,44 @@
+import zlib
+import struct
+import random
+import os
+
+def create_png(filename, text_key="key", text_value="value"):
+    def chunk(chunk_type, data):
+        chunk_len = struct.pack(">I", len(data))
+        chunk_crc = struct.pack(">I", zlib.crc32(chunk_type + data) & 0xffffffff)
+        return chunk_len + chunk_type + data + chunk_crc
+
+    png_signature = b"\x89PNG\r\n\x1a\n"
+    ihdr_data = struct.pack(">IIBBBBB", 1, 1, 8, 2, 0, 0, 0)  # width, height, bit depth, color type...
+    ihdr = chunk(b'IHDR', ihdr_data)
+
+    text_data = text_key.encode('latin1') + b'\x00' + text_value.encode('latin1')
+    text = chunk(b'tEXt', text_data)
+
+    idat_data = zlib.compress(b'\x00\xff\x00\x00')  # RGB pixel
+    idat = chunk(b'IDAT', idat_data)
+
+    iend = chunk(b'IEND', b'')
+
+    with open(filename, "wb") as f:
+        f.write(png_signature + ihdr + text + idat + iend)
+
+os.makedirs("seed", exist_ok=True)
+for i in range(20):
+    key = f"key{i}"
+    val = f"value{i}"
+    create_png(f"seed/fuzz{i}.png", key, val)
+
